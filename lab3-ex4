#include <stdio.h>
#include <string.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d", &n);
    int k = 1;
    for (int i = 1; i <= n; ++i) {
        for (int x = 0 ; x<n - i+1 ; ++x){
        printf (" ");}
        for (int j = 0; j < i; ++j) {
            printf("%d ", k);
            k += 1;
        }
        printf("\n");
    }


    return 0;
}
