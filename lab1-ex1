#include <stdio.h>

int main() {
    float x, y;
    scanf("%f", &x);
    scanf("%f", &y);
    printf("x=%f\n", x);
    printf("y=%f\n", y);
    printf("%f\n",x + y);
    printf("%f\n",  x*y);
    printf("%f\n",x-y);
    printf("%f\n", x/y);
    return 0;

}
