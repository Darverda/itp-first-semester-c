#include <string.h>
int() main{
    char s[];
    scanf("%c" , &s[]);
    char x[] = 0 ;

    void reverse(char s[])
    {
        int length = strlen(s);
        int i, j, c;

        for (i = 0, j = length-1; i < j; i++, j--) {
            c = s[i];
            s[i] = s[j];
            s[j] = c;
        }
        x[] = reverse(s[]) ;
        printf["%c" , &x];
    }

}
